{
  "name": "@fuse/next",
  "version": "0.1.0",
  "description": "The Fuse integration with Next.JS",
  "homepage": "https://github.com/StellateHQ/fuse.js",
  "bugs": "https://github.com/StellateHQ/fuse.js/issues",
  "license": "MIT",
  "author": "Stellate engineering <eng@stellate.co>",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/StellateHQ/fuse.js",
    "directory": "packages/next"
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/esm/client.js",
      "require": "./dist/client.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./rsc": {
      "types": "./dist/rsc.d.ts",
      "import": "./dist/rsc.mjs",
      "require": "./dist/rsc.js"
    },
    "./pages": {
      "types": "./dist/pages.d.ts",
      "import": "./dist/esm/pages.js",
      "require": "./dist/pages.js"
    }
  },
  "peerDependencies": {
    "@graphql-typed-document-node/core": "^3.0.0",
    "fuse": "^0.1.0",
    "next": ">=13.0.0"
  },
  "dependencies": {
    "@escape.tech/graphql-armor-block-field-suggestions": "^2.1.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@graphql-yoga/plugin-defer-stream": "^3.0.0",
    "@graphql-yoga/plugin-disable-introspection": "^2.0.0",
    "@urql/core": "^4.2.0",
    "graphql-yoga": "^5.0.0",
    "urql": "^4.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18",
    "@urql/next": "^1.1.0",
    "fuse": "workspace:^",
    "next": "14.0.3",
    "stellate": "^2.7.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
